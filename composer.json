{
    "name": "wmde/campaign-visitors-by-time",
    "description": "This plugin aggregates visitors per campaign/keyword in a more detailed interval of 15 minutes.",
    "license": "GPL-3.0-or-later",
    "type": "library",
    "require": {
        "php": ">=8.3"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.2",
        "slevomat/coding-standard": "~7.0",
        "phpmd/phpmd": "~2.11"
    },
    "autoload": {
        "psr-4": {
            "Matomo\\Plugins\\CampaignVisitorsByTime\\Test\\": "tests",
            "Matomo\\Plugins\\CampaignVisitorsByTime\\": ""
        }
    },
	"config": {
		"allow-plugins": {
			"composer/package-versions-deprecated": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
